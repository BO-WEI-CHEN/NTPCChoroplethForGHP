{
  "workbench.colorTheme": "monokai-chacoral (white)",
  "workbench.preferredDarkColorTheme": "dark-plus-syntax",
  "workbench.preferredHighContrastColorTheme": "Solarized Light",
  "powermode.enabled": true,
  "powermode.presets": "particles",

  // Jittering
  "powermode.enableShake": false,
  "powermode.backgroundMode": "mask",
  "powermode.explosionSize": 20,

  // about editor
  "editor.wordWrap": "on",
  "editor.fontFamily": "'Fira Code', monospace"
}
